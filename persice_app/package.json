{
  "name": "persice",
  "version": "1.0.0",
  "description": "Persice Frontend",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "live-server": "live-server",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist && npm run rimraf -- ../bekindred/static/assets/",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "postbuild:prod": "mv dist/*.js dist/*.map dist/*.gz dist/assets/js/ && cp -R dist/assets/ ../bekindred/static/assets/",
    "postbuild:dev": "mv dist/*.js dist/assets/js/ && cp -R dist/assets/ ../bekindred/static/assets/",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev-server.js --compress --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:mobile": "webpack-dev-server --config config/webpack.dev-server-mobile.js --compress --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev:hmr:mobile": "npm run server:dev:mobile -- --inline --hot",
    "serve:coverage": "http-server coverage/html-report-with-ts-source -o -p 8083",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "lint": "npm run tslint --force \"src/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "test": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "npm run typedoc -- --options typedoc.json --exclude \"**/*.spec.ts\" ./src/",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start:hmr:mobile": "npm run server:dev:hmr:mobile",
    "start:hmr:css": "concurrently --raw --kill-others \"npm run start:hmr\" \"gulp watch\"",
    "start:hmr:css:mobile": "concurrently --raw --kill-others \"npm run start:hmr:mobile\" \"gulp watch\"",
    "pretest": "rimraf coverage",
    "preversion": "npm test",
    "version": "npm run build",
    "jenkins": "karma start --reporters=dots,junit"
  },
  "author": "Sasa Macakanja <sasa.macakanja@toptal.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Persice/persice.com.git"
  },
  "homepage": "https://persice.com",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@ngrx/core": "1.0.1",
    "@ngrx/store": "2.0.1",
    "core-js": "^2.4.0",
    "dragula": "^3.6.8",
    "geocomplete": "^1.6.5",
    "hammerjs": "^2.0.8",
    "ie-shim": "^0.1.0",
    "lodash": "^4.13.1",
    "moment": "^2.10.6",
    "moment-timezone": "^0.5.4",
    "rxjs": "5.0.0-beta.6",
    "socket.io-client": "^1.4.5",
    "validate.js": "https://github.com/sasha7/validate.js.git",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.28",
    "@types/hammerjs": "^2.0.28",
    "@types/jasmine": "^2.2.29",
    "@types/lodash": "0.0.27",
    "@types/node": "^4.0.29",
    "@types/protractor": "1.5.16",
    "@types/selenium-webdriver": "2.44.26",
    "@types/socket.io-client": "^1.4.26",
    "@types/source-map": "^0.1.26",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.29",
    "angular2-hmr": "~0.8.1",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "~1.1.1",
    "browser-sync": "^2.11.1",
    "codelyzer": "~0.0.25",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "faker": "^3.0.1",
    "file-loader": "^0.9.0",
    "gulp": "*",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "*",
    "gulp-csslint": "^0.3.1",
    "gulp-cssnano": "^2.1.0",
    "gulp-jscs": "^4.0.0",
    "gulp-jshint": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jshint": "^2.9.1",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.7",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "live-server": "^1.0.0",
    "npm-run-all": "^2.3.0",
    "parse5": "^2.1.5",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.2.2",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "replace": "^0.3.0",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.0-beta",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
