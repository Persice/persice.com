{
  "version": "1.6.2",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "filesGlob": [
    "./**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "files": [
    "src/typings/_custom.d.ts",
    "src/app/components/app.ts",
    "src/app/components/leftnav/leftnav.ts",
    "src/app/components/logo/logo.ts",
    "src/app/bootstrap.ts"
  ]
}
