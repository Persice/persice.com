{
  "name": "persice",
  "version": "1.0.0",
  "description": "Angular 2 app for Persice",
  "main": "index.js",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached --progress --profile",
    "build:dev": " npm run env:dev  npm run webpack --colors --display-error-details --display-cached --progress --profile",
    "build:prod": "npm run env:prod npm run webpack --colors --display-error-details --display-cached --progress --profile",
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch",
    "server": "webpack-dev-server -d --hot --inline --colors --display-error-details --display-cached --port 8080",
    "express": "npm run env:dev node ./server/express-server.js",
    "express:dev": " npm run env:dev  node ./server/express-server.js",
    "express:prod": "npm run env:prod node ./server/express-server.js",
    "express-install": "npm install express connect-history-api-fallback morgan body-parser cors",
    "env:dev": "NODE_ENV=development",
    "env:prod": "NODE_ENV=production",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor",
    "e2e": "protractor",
    "ci": "npm run e2e && npm run test",
    "test": "karma start --reporters=mocha",
    "unit-tests": "karma start --reporters=dots,junit",
    "testwatch": "karma start --auto-watch --no-single-run --reporters=mocha",
    "testinbrowser": "live-server --open=src/public/unit-tests.html",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "prestart": "npm install",
    "start": "npm run server"
  },
  "author": "Sasa <sasa@meridian-studio.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/persice/persice.git"
  },
  "homepage": "https://persice.com",
  "dependencies": {
    "@reactivex/rxjs": "5.0.0-alpha.12",
    "angular2": "2.0.0-alpha.48",
    "geocomplete": "^1.6.5",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "reflect-metadata": "^0.1.2",
    "rx": "latest",
    "socket.io-client": "^1.3.7",
    "validate.js": "^0.9.0",
    "zone.js": "^0.5.8"
  },
  "devDependencies": {
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "jasmine-core": "2.3.4",
    "jasmine-jquery": "^2.1.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.8",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.7.0",
    "live-server": "0.8.1",
    "object-assign": "^4.0.1",
    "phantomjs": "^1.9.18",
    "phantomjs-bin": "^1.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tslint": "^3.1.1",
    "tslint-loader": "^1.0.2",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-bundle-tracker": "0.0.8",
    "webpack-dev-server": "^1.12.1",
    "webpack-notifier": "^1.2.1",
    "webpack-path-rewriter": "^1.1.2"
  }
}
